{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/platform-api/src",
  "monorepo": true,
  "root": "apps/platform-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/platform-api/tsconfig.app.json"
  },
  "projects": {
    "platform-api": {
      "type": "application",
      "root": "apps/platform-api",
      "entryFile": "main",
      "sourceRoot": "apps/platform-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/platform-api/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "storage-service": {
      "type": "application",
      "root": "apps/storage-service",
      "entryFile": "main",
      "sourceRoot": "apps/storage-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/storage-service/tsconfig.app.json"
      }
    },
    "migrations-runner": {
      "type": "application",
      "root": "apps/migrations-runner",
      "entryFile": "main",
      "sourceRoot": "apps/migrations-runner/src",
      "compilerOptions": {
        "tsConfigPath": "apps/migrations-runner/tsconfig.app.json"
      }
    }
  }
}
